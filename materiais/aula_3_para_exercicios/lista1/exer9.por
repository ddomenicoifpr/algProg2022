algoritmo
declare i, num1, num2, multiplicacao numerico
escreva "Informe o primeiro número: "
leia num1
escreva "Informe o segundo número: "
leia num2
multiplicacao <- 0
para i <-1 ate num2 faca
inicio
	multiplicacao <- multiplicacao + num1
fim
escreva "Multiplicação de ", num1, " por ", num2, " = ", multiplicacao
fim_algoritmo
