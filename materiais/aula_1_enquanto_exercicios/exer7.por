algoritmo
declare valor, soma_valor, conta numerico
conta <- 1
soma_valor <- 0
enquanto conta <= 5 faca
inicio
	escreva "Informe o valor de contribuição do aluno R$: "
	leia valor
	soma_valor <- soma_valor + valor
	
	conta <- conta + 1
fim
escreva "O valor total arrecadado é R$ ", soma_valor
fim_algoritmo
