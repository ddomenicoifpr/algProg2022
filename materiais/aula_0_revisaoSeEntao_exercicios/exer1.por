algoritmo
declare 
	num1, num2 numerico
escreva "Informe o primeiro número: "
leia num1
escreva "Informe o segundo número: "
leia num2
se num1 = num2 entao
	escreva "Números iguais"
senao
inicio
	se num1 > num2 entao
		escreva num1, " é o maior número"
	senao
		escreva num2, " é o maior número"
fim
fim_algoritmo
